  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },

  "extensions": {
    "durableTask": {
      "hubName": "InvoiceOrchestrationHub"
